                  <Image url="@../icons/cancel.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="refresh" disable="true" mnemonicParsing="false">
         <graphic>
            <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
